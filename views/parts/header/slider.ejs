<div id="carouselExampleCaptions" class="carousel slide" data-bs-ride="carousel">
  <div class="carousel-inner">


    <% const item = sliders[0]  %>

    <div class="carousel-item">

      <img src="/images/slider/<%= item.img %>" class="d-block w-100" alt="..." />
      <div class="carousel-caption top-50 left=50 translate-middle-y">
        <h2>
          <%= item.title %>
        </h2>
      </div>


    </div>
  </div>

</div>